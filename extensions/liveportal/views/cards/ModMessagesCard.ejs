<div id="ModMessagesCard" class="card bg-light text-dark">
    <!--img class=" card-img-top" src="..." alt="Card image cap" -->
    <div class="card-body px-1">
        <div id="ChatMessagesCardTitle" class="card-title">Mod Messages</div>
        <!-- div class="" -->
        <!-- style="overflow-y: auto;  max-height: 300px" -->
        <div id="ModMessagesCardText" style="overflow-y: auto; height: 10vh;">
        </div>
        <form>
            <div id="ModMessageGroup" class="input-group">
                <input id="modInputText" class="form-control card bg-light text-dark border-secondary" type="text"
                    placeholder="">
                <span class="input-group-btn">
                    <button class="btn btn-secondary" for="modInputText" type="submit"
                        onclick="modInput(event)">Chat</button></span>
            </div>
        </form>
    </div>
</div>