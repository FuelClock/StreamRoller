name: test
run-name: ${{ github.actor }} is delivering
on: workflow_dispatch
env:
  SRS_GITHUB_REF_NAME: ${{ github.GITHUB_REF_NAME }}  
  MY_API_TOKEN_GITHUB: ${{secrets.API_TOKEN_GITHUB}}
jobs:
  testing:
    runs-on: windows-latest
    steps:
      - name: checkout code
        uses: actions/checkout@v3
        with:
          path: "sr_source"
      - name: try to delete dirs
        run: |
            dir
            echo "var = ${{emv.SRS_GITHUB_REF_NAME }}"
            rmdir --help
            dir
            
