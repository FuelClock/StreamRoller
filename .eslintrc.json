{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": "eslint:recommended",
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
      "no-unused-vars": "off",
      "no-unexpected-multiline": "off",
      "no-prototype-builtins": "off",
      "no-control-regex": "off"
    },
    "overrides": [
    {
      "files": [ "./**/*.js" ]
      
    }
  ],
  "ignorePatterns": ["node_modules", "src/frontend/generated/*"]
}
